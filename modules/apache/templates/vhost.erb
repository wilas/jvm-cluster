<VirtualHost *:80>

    ServerName <%= vhost_domain %>
    ServerAlias www.<%= vhost_domain %>
    ServerAdmin admin@<%= vhost_domain %>

    DocumentRoot <%= vhost_root %>
    ErrorLog logs/<%= vhost_domain %>-error_log
    CustomLog logs/<%= vhost_domain %>-access_log common

    <Directory <%= vhost_root %>>
        Allow from all
        Options +Includes +Indexes +FollowSymLinks
        AllowOverride all
    </Directory>

    #RewriteEngine On
    #RewriteRule ^(.*$) https://kkk.pl$1

</VirtualHost>

